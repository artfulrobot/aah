<?xml version="1.0"?>
<extension key="aah" type="module">
  <file>aah</file>
  <name>Aah theme</name>
  <description>A kindly theme with Bootstrap support</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Rich Lott / Artful Robot</author>
    <email>code.commits@peopleandplanet.org</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/artfulrobot/aah</url>
    <url desc="Documentation">https://github.com/artfulrobot/aah</url>
    <url desc="Support">https://github.com/artfulrobot/aah/issues</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2024-02-22</releaseDate>
  <version>1.0.2</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>5.64</ver>
  </compatibility>
  <comments>This is my take on providing a modern and functionally useful theme for CiviCRM</comments>
  <civix>
    <namespace>CRM/Aah</namespace>
    <format>23.02.1</format>
  </civix>
  <mixins>
    <mixin>menu-xml@1.0.0</mixin>
    <mixin>theme-php@1.0.0</mixin>
    <mixin>smarty-v2@1.0.1</mixin>
  </mixins>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
</extension>
